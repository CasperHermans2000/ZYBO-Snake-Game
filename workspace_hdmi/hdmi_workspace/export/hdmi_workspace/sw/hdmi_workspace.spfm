<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="hdmi_workspace"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
hdmi_workspace
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="hdmi_workspace">
        <sdx:configuration sdx:name="hdmi_workspace"
                           sdx:displayName="hdmi_workspace"
                           sdx:defaultProcessorGroup="standalone_domain"
                           sdx:runtimes="cpp">
            <sdx:description>hdmi_workspace</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="hdmi_workspace/boot/hdmi_workspace.bif"
                            sdx:readme="hdmi_workspace/boot/generic.readme"
                            sdx:qemuArguments="hdmi_workspace/qemu/qemu_args.txt"
                            />
                <sdx:image sdx:name="standalone_domain"
                            sdx:bif="hdmi_workspace/boot/hdmi_workspace.bif"
                            sdx:readme="hdmi_workspace/boot/generic.readme"
                            sdx:qemuArguments="hdmi_workspace/standalone_domain/qemu/qemu_args.txt"
                            />
            </sdx:bootImages>
            <sdx:processorGroup sdx:name="standalone_domain"
                                sdx:displayName="standalone on ps7_cortexa9_0"
                                sdx:cpuType="cortex-a9"
                                sdx:cpuInstance="ps7_cortexa9_0">
                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="hdmi_workspace/standalone_domain/bsplib/lib"
                        sdx:bspIncludePaths="hdmi_workspace/standalone_domain/bspinclude/include"
                        sdx:bspConfig="hdmi_workspace/standalone_domain/system.mss"
                />
            </sdx:processorGroup>
        </sdx:configuration>
    </sdx:systemConfigurations>
</sdx:platform>
